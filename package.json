{
  "name": "ori-auth",
  "version": "1.0.0",
  "description": "Ori-Auth is a simple TypeScript npm package designed to handle Firebase authentication. It offers a set of easy-to-use functions for creating users, retrieving user data, logging in, resetting passwords, and logging out.",
  "main": "index.js",
  "types": "./build/Index.d.ts",
  "exports": {
    ".": {
      "import": "./build/Index.js",
      "require": "./build/Index.js"
    }
  },
  "scripts": {
    "dev": "rimraf ./build && tsc -w",
    "build": "rimraf ./build && tsc"
  },
  "files": [
    "assets/**",
    "build/**",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.5.1",
    "prompt": "^1.3.0",
    "tslib": "^2.6.2"
  },
  "keywords": [
    "firebase",
    "authentication",
    "user management",
    "firebase authentication",
    "user authentication",
    "password reset",
    "npm package",
    "node.js",
    "typescript",
    "user login",
    "user creation",
    "firebase auth",
    "firebase user",
    "user data",
    "user logout",
    "firebase config",
    "user authentication management"
  ],
  "author": "Cosmic-fi",
  "license": "CCANC",
  "bugs": {
    "url": "https://github.com/cosmic-fi/Ori-Core-Java/issues"
  },
  "homepage": "https://github.com/cosmic-fi/Ori-Core-Java#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:cosmic-fi/Ori-Core-Java.git"
  },
  "devDependencies": {
    "adm-zip": "^0.5.10",
    "node": "^21.0.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
